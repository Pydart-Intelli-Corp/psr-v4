{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.EntityFrameworkCore": "Error"
    },
    "Console": {
      "FormatterName": "json"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=PROD_SERVER_IP;Port=3306;Database=psr_machine_api;User=PROD_DB_USER;Password=PROD_DB_PASSWORD;SslMode=Required;MinimumPoolSize=5;MaximumPoolSize=100;",
    "MainDatabase": "Server=PROD_SERVER_IP;Port=3306;Database=psr_v4_main;User=PROD_DB_USER;Password=PROD_DB_PASSWORD;SslMode=Required;MinimumPoolSize=5;MaximumPoolSize=100;"
  },
  "Jwt": {
    "Secret": "PROD_JWT_SECRET_256_BITS_OR_MORE",
    "ExpirationDays": 7
  },
  "MachineSettings": {
    "EnableAutoDispatch": false
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "PermitLimit": 100,
    "WindowSeconds": 60
  },
  "Cors": {
    "AllowedOrigins": ["https://your-production-domain.com", "https://app.poornasree.com"]
  }
}
